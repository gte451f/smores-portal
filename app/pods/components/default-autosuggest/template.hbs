<div class="typeahead-wrap">
    {{input class=inputClazz value=selectedValue placeholder=placeHolderText autocomplete="off" spellcheck="false"}}
    <span class="tt-dropdown-menu" {{bind-attr style=visibility}}>
  <div class="tt-dataset-states">
    <span class="tt-suggestions">
        {{#each suggestions  as |item|}}
            <div {{bind-attr class=":tt-suggestion result.highlight:tt-cursor"}} {{action "selectItem" item }}>
                {{yield item}}
            </div>
        {{else}}
            <div class="message">
                <p>{{noMesssagePlaceHolderText}}</p>
            </div>
        {{/each}}
    </span>
  </div>
</span>
</div>
